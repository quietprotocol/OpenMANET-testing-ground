# Default device configuration for OpenMANET deployment scripts
# Copy this file to .env and update with your actual values
# The .env file is gitignored and will not be committed

# Device IP address
DEVICE_IP=192.168.1.1

# Device credentials
DEVICE_USER=root
DEVICE_PASS=your-password-here

# Build server configuration (for OpenWrt image transfer)
BUILD_SERVER_USER=ubuntu
BUILD_SERVER_HOST=your-build-server.example.com
BUILD_SERVER_PATH=/path/to/openwrt/bin/targets/bcm27xx/bcm2711/
BUILD_SSH_KEY=~/.ssh/your-ssh-key
BUILD_LOCAL_PATH=~/path/to/firmware/destination/
